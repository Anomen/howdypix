{
  "name": "@howdypix/server",
  "version": "0.1.2-alpha.0",
  "description": "Server for the Photo Gallery",
  "repository": {
    "url": "git@github.com:vickev/howdypix.git"
  },
  "main": "index.js",
  "author": "Kevin Vicrey <kevin.vicrey@nexu.org>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "typings": "",
  "bin": {
    "howdypix-server": "./bin/howdypix-server.js"
  },
  "files": [
    "dist",
    "config/custom-environment-variables.json",
    "config/default.yaml",
    "bin",
    "ormconfig.js"
  ],
  "scripts": {
    "start": "node dist/index",
    "start:mock": "MOCK=true node dist/index",
    "dev": "yarn build && DEBUG=app:*,-lib:* yarn watch",
    "dev:mock": "MOCK=true yarn dev",
    "watch": "nodemon dist/index",
    "build": "babel src --out-dir dist --extensions '.ts' --verbose",
    "lint": "eslint {src,e2e}/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:e2e": "concurrently 'yarn start:mock' 'wait-on tcp:3004 && yarn test:e2e:local' --kill-others --kill-others-on-fail --raw --success first",
    "test:e2e:local": "NODE_ENV=e2e jest --passWithNoTests --forceExit"
  },
  "dependencies": {
    "@howdypix/graphql-schema": "^0.1.0",
    "@howdypix/shared-types": "^0.1.0",
    "@howdypix/utils": "^0.1.0",
    "amqplib": "^0.5.5",
    "apollo-server": "^2.25.0",
    "apollo-server-express": "^2.25.0",
    "chokidar": "^3.5.1",
    "config": "^3.3.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-type": "^14.6.0",
    "globby": "^11.0.3",
    "graphql": "^14.6.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mjml": "^4.9.3",
    "nexus": "^0.11.7",
    "nodemailer": "^6.6.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.2.0",
    "strict-event-emitter-types": "^2.0.0",
    "typeorm": "^0.2.32",
    "wait-on": "^5.3.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@howdypix/babel-preset": "^0.1.0",
    "@howdypix/eslint-config": "^0.1.0",
    "@howdypix/jest-config": "^0.1.0",
    "@howdypix/typescript-config": "^0.1.0",
    "@types/amqplib": "^0.5.17",
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.12",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.170",
    "@types/mjml": "^4.7.0",
    "@types/node-fetch": "^2.5.10",
    "@types/nodemailer": "^6.4.2",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/yaml": "^1.9.7",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.19.2",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "graphql-tag": "^2.12.4",
    "jest": "^25.2.7",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "typescript": "^3.9.3",
    "wait-for-expect": "^3.0.2"
  }
}
