    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "globby": "^10.0.1",
    "graphql": "^14.5.5",
    "lodash": "^4.17.15",
    "nexus": "^0.11.7",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.0",
    "strict-event-emitter-types": "^2.0.0",
    "typeorm": "^0.2.19",
    "wait-on": "^3.3.0",
    "yaml": "^1.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
