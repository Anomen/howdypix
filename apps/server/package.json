{
  "name": "@howdypix/server",
  "version": "0.1.0-alpha.0",
  "description": "Server for the Photo Gallery",
  "repository": {
    "url": "git@github.com:vickev/howdypix.git"
  },
  "main": "index.js",
  "author": "Kevin Vicrey <kevin.vicrey@nexu.org>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "typings": "",
  "bin": {
    "howdypix-server": "./bin/howdypix-server.js"
  },
  "files": [
    "dist",
    "config",
    "bin",
    "ormconfig.json"
  ],
  "scripts": {
    "start": "node dist/index",
    "start:mock": "MOCK=true node dist/index",
    "dev": "yarn build && DEBUG=app:*,-lib:* yarn watch",
    "dev:mock": "MOCK=true yarn dev",
    "watch": "yarn build --skip-initial-build --watch & nodemon dist/index",
    "build": "babel src --out-dir dist --extensions '.ts' --verbose",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:e2e": "concurrently 'yarn start:mock' 'wait-on tcp:3004 && yarn test:e2e:local' --kill-others --kill-others-on-fail --raw --success first",
    "test:e2e:local": "NODE_ENV=e2e jest --passWithNoTests"
  },
  "dependencies": {
    "@howdypix/graphql-schema": "^0.0.3-alpha.0",
    "@howdypix/shared-types": "^0.0.3-alpha.0",
    "@howdypix/utils": "^0.0.3-alpha.4",
    "amqplib": "^0.5.5",
    "apollo-server": "^2.9.3",
    "apollo-server-express": "^2.9.7",
    "chokidar": "^3.1.1",
    "config": "^3.2.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "globby": "^10.0.1",
    "graphql": "^14.5.5",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mjml": "^4.5.1",
    "nexus": "^0.11.7",
    "nodemailer": "^6.3.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.0",
    "strict-event-emitter-types": "^2.0.0",
    "typeorm": "^0.2.19",
    "wait-on": "^4.0.1",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@howdypix/babel-preset": "^0.0.3-alpha.0",
    "@howdypix/eslint-config": "^0.0.3-alpha.0",
    "@howdypix/graphql-schema": "^0.0.1",
    "@howdypix/jest-config": "^0.0.3-alpha.2",
    "@howdypix/typescript-config": "^0.0.3-alpha.0",
    "@types/amqplib": "^0.5.13",
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.36",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.138",
    "@types/mjml": "^4.0.2",
    "@types/nodemailer": "^6.2.2",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/yaml": "^1.0.2",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "eslint": " ^6.3.0",
    "graphql-tag": "^2.10.1",
    "jest": "^24.9.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.2",
    "typescript": "^3.7.2"
  }
}
