    "@types/amqplib": "^0.5.13",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/yaml": "^1.0.2",
    "eslint": " ^6.3.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "typescript": "^3.6.2"
  }
