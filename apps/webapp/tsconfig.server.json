{
  "extends": "@howdypix/typescript-config/base",
  "exclude": ["node_modules", "**/*.test.ts", "server/mock/**/*.ts"],
  "include": ["server/**/*.ts"],
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "outDir": "dist",
    "target": "es2017",
    "isolatedModules": false,
    "noEmit": false
  }
}
